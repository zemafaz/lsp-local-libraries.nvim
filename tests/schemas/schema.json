{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "Your name.",
            "example": [
                "André",
                "Armando",
                "José",
                "Pedro"
            ]
        },
        "age": {
            "type": "number",
            "description": "Your age."
        },
    },
    "required": ["name"]
}
